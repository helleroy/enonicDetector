<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean class="no.kaedeno.enonic.detector.Detector">
		<property name="priority" value="1" />
		<property name="urlPattern" value=".*" />
		<property name="displayName" value="Detector interceptor" />
		<property name="environment" ref="plugin.service.pluginEnvironment" />
		<property name="context" ref="plugin.context" />
		<property name="pluginConfig" ref="plugin.config" />
	</bean>

</beans>